<GameData>

	<!-- [Updated UA] Nobel Prize: +25% Great Person points in all Cities. Gain Science whenever a Great Person is born. -->
	<Traits>
		<Update>
			<Set GreatPersonGiftInfluence="0" DOFGreatPersonModifier="0" GreatPeopleRateModifier="25" SciencePerGreatPersonBorn="37" />
			<Where Type="TRAIT_DIPLOMACY_GREAT_PEOPLE" />
		</Update>
	</Traits>
	
	<!-- This piece of text super wonky because Hakkapeliitta is now a French Cuirassier unit. It works though, don't recommend messing with it. -->
	<Civilization_UnitClassOverrides>
		<Delete UnitType="UNIT_SWEDISH_CAROLEAN" />
		<Update>
			<Set UnitClassType="UNITCLASS_MUSKETMAN" UnitType="UNIT_SWEDISH_CAROLEAN" />
			<Where CivilizationType="CIVILIZATION_SWEDEN" />
		</Update>
	</Civilization_UnitClassOverrides>
	
