<GameData>
	
	<Defines>

		<!-- Cultural City States: Now provide +2/4 in Ancient, +5/8 in Medieval, and +10/15 in Industrial (down from +3/6, +6/12, +13/26) based on Friends/Allies status. -->
		<Update>
			<Set Value="2" />
			<Where Name="FRIENDS_CULTURE_BONUS_AMOUNT_ANCIENT" />
		</Update>
		<Update>
			<Set Value="2" />
			<Where Name="ALLIES_CULTURE_BONUS_AMOUNT_ANCIENT" />
		</Update>
		<Update>
			<Set Value="5" />
			<Where Name="FRIENDS_CULTURE_BONUS_AMOUNT_MEDIEVAL" />
		</Update>
		<Update>
			<Set Value="3" />
			<Where Name="ALLIES_CULTURE_BONUS_AMOUNT_MEDIEVAL" />
		</Update>
		<Update>
			<Set Value="10" />
			<Where Name="FRIENDS_CULTURE_BONUS_AMOUNT_INDUSTRIAL" />
		</Update>
		<Update>
			<Set Value="5" />
			<Where Name="ALLIES_CULTURE_BONUS_AMOUNT_INDUSTRIAL" />
		</Update>

		<!-- Maritime City States -
		
		Pre Reneissance:
			Friends: +2 in Capital
			Allies: +4 Food in Capital and +1 Food in all other Cities
			
		Post Reneissance:
			Friends: +3 Food in Capital and +1 Food in all other Cities
			Allies: +5 Food in Capital and +2 Food in all other Cities
			
		"Regardless of Era, Compared to Friends status, Allied status always gives additional +2 Food in Capital and +1 Food in other Cities."
		
		-->
		
		<Update>
			<Set Value="100" />
			<Where Name="ALLIES_OTHER_CITIES_FOOD_BONUS_AMOUNT" />
		</Update>
		<Update>
			<Set Value="100" />
			<Where Name="FRIENDS_OTHER_CITIES_FOOD_BONUS_AMOUNT_POST_RENAISSANCE" />
		</Update>
		<Update>
			<Set Value="100" />
			<Where Name="ALLIES_CAPITAL_FOOD_BONUS_AMOUNT" />
		</Update>
		<Update>
			<Set Value="200" />
			<Where Name="FRIENDS_CAPITAL_FOOD_BONUS_AMOUNT_PRE_RENAISSANCE" />
		</Update>
		<Update>
			<Set Value="200" />
			<Where Name="FRIENDS_CAPITAL_FOOD_BONUS_AMOUNT_POST_RENAISSANCE" />
		</Update>
		
		<!-- Religious City-States
		
		Ancient and Classical:
			Friends: 2 Faith
			Allies: 4 Faith
			
		Medieval and Reneissance:
			Friends: 6 Faith
			Allies: 12 Faith
			
		Industrial and beyond:
			Friends: 12 Faith
			Allies: 24 Faith
			
		"Faith bonuses are the originals times three when in Medieval and Reneissance, and times six in Industrial and beyond."
		
		-->

		<Update>
			<Set Value="6" />
			<Where Name="FRIENDS_FAITH_FLAT_BONUS_AMOUNT_MEDIEVAL" />
		</Update>
		<Update>
			<Set Value="6" />
			<Where Name="FRIENDS_FAITH_FLAT_BONUS_AMOUNT_RENAISSANCE" />
		</Update>
		<Update>
			<Set Value="6" />
			<Where Name="ALLIES_FAITH_FLAT_BONUS_AMOUNT_MEDIEVAL" />
		</Update>
		<Update>
			<Set Value="6" />
			<Where Name="ALLIES_FAITH_FLAT_BONUS_AMOUNT_RENAISSANCE" />
		</Update>
		<Update>
			<Set Value="12" />
			<Where Name="FRIENDS_FAITH_FLAT_BONUS_AMOUNT_INDUSTRIAL" />
		</Update>
		<Update>
			<Set Value="12" />
			<Where Name="ALLIES_FAITH_FLAT_BONUS_AMOUNT_INDUSTRIAL" />
		</Update>
	</Defines>

</GameData>